          </div>
          <div class="clear"></div>
            <!--Popular Stories End-->
            <div class="module-title">Top News</div>
            <div class="module-inner">
              {% for n in politicsnews %}
              <div class="videorow">
                <div class="news-item">
                  <img class="news-graphic" src="http://newscdn.lycos.com/{{n.thumb_image}}"/>
                  <p>{{n.title}}<br /><span class="uploaded-date"> {{n.upload_date}} </span></p>
                </div>
              </div>
              {% endfor %}
              <div class="videorow">
              </div>

