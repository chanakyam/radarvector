    <div class="container2">
      <article>
        <aside class="width67">
          <div class="clear"></div>
          <div class="module-title"></div>
            <div class="module-inner">
              {% for t in titles %}
              <div class="row devider" >
                <a href="/n/{{ t.id }}">{{ t.description }}</a><span class="uploaded-date"> - {{ t.upload_date}}</span>
              </div>
              {% endfor %}
              <!-- <div class="row">
                <pagination on-select-page="pageChanged(page)" items-per-page="newsPerPage" total-items="bigTotalItems" page="bigCurrentPage" max-size="maxSize" boundary-links="true" rotate="false" num-pages="numPages"></pagination>
              </div> -->
            </div>
        </aside>

        <aside class="width30 margin20_L">

          <span class="clearboth"></span>

          <div class="module margin10_T RgtTopNews">
            <div class="module popular">
            <div class="module-title">Popular Stories</div>
            <div class="module-inner">
              {% for lvid in popularvideos %}
              <div class="row videorow">
                <span class="width40">
                  <a href="/v/{{lvid.id}}">
                    <img src="http://newscdn.lycos.com/world_news/{{lvid.thumb_image}}" width="97" height="75"/>
                    <img class="smallplay" src="/images/play-icon.png">
                  </a>
                </span>
                <span class="width60">
                  <p><a href="/v/{{lvid.id}}">{{lvid.title}}</a></p>
                  <span class="uploaded-date"> {{lvid.upload_date}} </span>
                </span>
              </div>
              {% endfor %}
              <a href="/videos?p=1" class="more">More Stories</a>
            </div>
          </div>
          <div class="clear"></div>
          <div class="rt-btadd margin15_T">
            <!--/* Switch v3.8.5 - Radarvector.com - Radarvector.com - 300x250 - 300x250 */-->
